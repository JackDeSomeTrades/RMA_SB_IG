<robot
  name="gripper_lateral_frame"
  xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- extend xacro functionality with macros of urdf_utils -->
  <xacro:include filename="$(find urdf_utils)/urdf/properties/defaults.xacro" />
  <xacro:include filename="$(find urdf_utils)/urdf/limit.xacro" />

  <!-- Load default joint data from yaml files in config directory. -->
  <!-- If additional data is present in cman, use it to overwrite the default one. -->
  <xacro:property
      name="cman_joints"
      value="${load_cman('conveyor_gripper_1_0_description', 'conveyor_gripper_1_0_description.yaml', '')}" />

  <!-- start of manually added component -->
  <joint
    name="gripper_base_x_to_lateral_frame"
    type="fixed">
    <origin
      xyz="0.16682 0.00254 0.09900"
      rpy="0 0 0" />
    <parent
      link="gripper_base_x_link" />
    <child
      link="gripper_lateral_frame_link" />
    <axis
      xyz="0 0 0" />
  </joint>

  <!-- intersection between left/right foremost point axis and gripper_y XY plane -->
  <link
    name="gripper_surface_tip_link" />
  <joint
    name="gripper_lateral_base_to_surface_tip"
    type="fixed">
    <origin
      xyz="0.5286 0 0.09824"
      rpy="0 0 0" />
    <parent
      link="gripper_lateral_frame_link" />
    <child
      link="gripper_surface_tip_link" />
  </joint>

  <link
    name="barcode_search_frame" />
  <joint
    name="barcode_search_joint"
    type="fixed">
    <origin
      xyz="0.1 0 -0.03"
      rpy="0 0 0" />
    <parent
      link="gripper_surface_tip_link" />
    <child
      link="barcode_search_frame" />
  </joint>

